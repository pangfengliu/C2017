#include<stdio.h>
#include"placement.h"
 
int main(){
  int N,M,method;
  scanf("%d%d%d",&N,&M,&method);
  int buckets[N];
  for(int i=0;i<N;i++)
    scanf("%d",&buckets[i]);
  int balls[M];
  for(int i=0;i<M;i++)
    scanf("%d",&balls[i]);
  int result[M];
  place(buckets,N,balls,M,method,result);
  for(int i=0;i<M;i++)
    printf("%d%s",result[i],i==M-1?"":" ");
  return 0;
}
